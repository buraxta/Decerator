def d1(func):
    def _d1(*args, **kwargs):
        print(f"d1 here for {func.__name__}")
        print("A")
        func(*args, **kwargs)
        print("B")
    return _d1

def d2(func):
    def _d2(*args, **kwargs):
        print(f"d2 here for {func.__name__}")
        print("C")
        func(*args, **kwargs)
        print("D")
    return _d2

@d1
@d2
def fd(x):
    print(f"f says {x}")

fd("Bora")
